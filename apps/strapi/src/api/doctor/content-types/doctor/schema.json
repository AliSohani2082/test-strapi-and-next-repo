{
  "kind": "collectionType",
  "collectionName": "doctors",
  "info": {
    "singularName": "doctor",
    "pluralName": "doctors",
    "displayName": "Doctor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "firstName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "lastName": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "email": {
      "type": "email",
      "required": false
    },
    "phone": {
      "type": "biginteger"
    },
    "address": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "about": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "education": {
      "type": "component",
      "repeatable": true,
      "component": "utilities.text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "certifications": {
      "type": "component",
      "repeatable": true,
      "component": "utilities.text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "services": {
      "type": "component",
      "repeatable": true,
      "component": "utilities.text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "speciality": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "uid",
      "targetField": "lastName",
      "required": true
    },
    "fullEnglishName": {
      "type": "string",
      "required": true,
      "private": true
    },
    "image": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": false
    },
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "mappedBy": "author"
    }
  }
}
